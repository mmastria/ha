        </oneText>
        <oneText name="FILTER_SLOT_NAME_2">
      L
        </oneText>
        <oneText name="FILTER_SLOT_NAME_3">
      R
        </oneText>
        <oneText name="FILTER_SLOT_NAME_4">
      G
        </oneText>
        <oneText name="FILTER_SLOT_NAME_5">
      B
        </oneText>
        <oneText name="FILTER_SLOT_NAME_6">
      HA
        </oneText>
        <oneText name="FILTER_SLOT_NAME_7">
      OIII
        </oneText>
    </newTextVector>
    <newSwitchVector device="SX Wheel" name="USEJOYSTICK">
        <oneSwitch name="ENABLE">
Off
        </oneSwitch>
        <oneSwitch name="DISABLE">
On
        </oneSwitch>
    </newSwitchVector>
    <newTextVector device="SX Wheel" name="JOYSTICKSETTINGS">
        <oneText name="Change Filter">
JOYSTICK_1
        </oneText>
        <oneText name="Reset">
BUTTON_1
        </oneText>
    </newTextVector>
</INDIDriver>
